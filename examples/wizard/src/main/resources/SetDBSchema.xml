<?xml version="1.0" encoding="windows-1252"?>

<panel resizable="false" title="DB Schema"  >

<vbox gap="20">

	<hbox border="TitledBorder(XML)" >
	<textfield columns="20" bindWith="selectedFile" editable="false"/>
	<button text="choose" action="selectFile"/>
	</hbox>
	
	<checkbox text="generate SQL" bindWith="generateSQL" />
	<checkbox text="drop Tables" bindWith="dropTables" />
	<checkbox text="continue on Error" bindWith="continueOnError" />
	
</vbox>

</panel>
