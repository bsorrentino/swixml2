<?xml version="1.0" encoding="windows-1252"?>

<dialog 
    minimumSize="400,240"
	resizable="false" 
	title="Outbound" 
	undecorated="false" 
	alwaysOnTop="true"
	defaultCloseOperation="JFrame.HIDE_ON_CLOSE" 
	modal="false"
	>

<vbox gap="20">


	<hbox border="CompoundBorder(TitledBorder(Interazioni attive), EmptyBorder(5,5,10,15))" font="Arial-BOLD-11">	
	<combobox id="cmbChain" bindList="${outboundChainList}" action="selectChain" editable="false"/>
	</hbox>

	<hbox gap="5" border="CompoundBorder(TitledBorder(Informazioni esito), EmptyBorder(5,5,10,5))" >
		<label text="Esito:" />
		<combobox  id="cmbCallResult" bindList="${callResultList}" action="selectCallResult"  font="Arial-BOLD-11" editable="false" />
		<box.hglue/>
		<checkbox id="chkTreatment" text="personal" bindWith="personal" visible="false"/>
	</hbox>

	<!--  box.vstrut height="5"/-->
	
	<hbox>
		<button id="btnOK" Text="Conferma Esito" action="submit"/>
	</hbox>
	
    
</vbox>

</dialog>
